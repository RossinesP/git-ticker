# LLM Provider Configuration
# Set LLM_PROVIDER to "anthropic" or "openai" to choose the LLM provider
LLM_PROVIDER=anthropic

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# LangSmith Observability Configuration
# Get your API key from https://smith.langchain.com/
LANGCHAIN_API_KEY=your_langsmith_api_key_here
# Enable LangSmith tracing (set to "true" to enable)
LANGCHAIN_TRACING_V2=true
# Optional: Set a project name in LangSmith (defaults to "default")
LANGCHAIN_PROJECT=gitlab-ticker
