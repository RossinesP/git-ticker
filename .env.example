# Anthropic API Configuration
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=

# LLM Provider Configuration
# Options: 'anthropic', 'claude', 'openai', 'gpt' (default: 'anthropic')
LLM_PROVIDER=anthropic

# Anthropic Configuration (required if LLM_PROVIDER is 'anthropic' or 'claude')
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# OpenAI Configuration (required if LLM_PROVIDER is 'openai' or 'gpt')
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4-turbo-preview
